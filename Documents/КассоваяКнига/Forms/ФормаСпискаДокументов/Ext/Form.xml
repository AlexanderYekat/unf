			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ Разрешенные
	Документ.Ссылка,
	Документ.ПометкаУдаления,
	Документ.Номер,
	Документ.Дата,
	Документ.Проведен,
	Документ.Организация КАК Организация,
	Документ.ДоговорКонтрагента КАК ДоговорКонтрагента,
	Документ.Касса КАК Касса,
	Документ.Ответственный,
	Документ.СуммаПоступления,
	Документ.СуммаВыдачи,
	Документ.НомераЛистов,
	Документ.Представление,
	Документ.МоментВремени,
	Документ.Комментарий,
	ГОД(Документ.Дата) КАК Год,
	ВЫБОР
		КОГДА Документ.Касса = ЗНАЧЕНИЕ(Справочник.Кассы.ПустаяСсылка)
			ТОГДА ДенежныеОстаткиОрганизации.СуммаНачальныйОстаток
		ИНАЧЕ ДенежныеОстаткиКассы.СуммаНачальныйОстаток
	КОНЕЦ КАК СуммаНачальныйОстаток,
	Документ.СуммаПоступления КАК СуммаПриход,
	Документ.СуммаВыдачи КАК СуммаРасход,
	ВЫБОР
		КОГДА Документ.Касса = ЗНАЧЕНИЕ(Справочник.Кассы.ПустаяСсылка)
			ТОГДА ДенежныеОстаткиОрганизации.СуммаКонечныйОстаток
		ИНАЧЕ ДенежныеОстаткиКассы.СуммаКонечныйОстаток
	КОНЕЦ КАК СуммаКонечныйОстаток,
	ВЫБОР
		КОГДА Документ.Касса = ЗНАЧЕНИЕ(Справочник.Кассы.ПустаяСсылка)
			ТОГДА ДенежныеОстаткиОрганизации.СуммаНачальныйОстаток + Документ.СуммаПоступления - Документ.СуммаВыдачи - ДенежныеОстаткиОрганизации.СуммаКонечныйОстаток
		ИНАЧЕ ДенежныеОстаткиКассы.СуммаНачальныйОстаток + Документ.СуммаПоступления - Документ.СуммаВыдачи - ДенежныеОстаткиКассы.СуммаКонечныйОстаток
	КОНЕЦ КАК ДельтаОшибки
ИЗ
	Документ.КассоваяКнига КАК Документ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ДенежныеСредства.ОстаткиИОбороты(, , Авто, , ТипДенежныхСредств = Значение(Перечисление.ТипыДенежныхСредств.Наличные)) КАК ДенежныеОстаткиОрганизации
		ПО (НАЧАЛОПЕРИОДА(Документ.Дата, ДЕНЬ) = ДенежныеОстаткиОрганизации.ПериодДень)
			И Документ.Организация = ДенежныеОстаткиОрганизации.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ДенежныеСредства.ОстаткиИОбороты(, , Авто, , ТипДенежныхСредств = Значение(Перечисление.ТипыДенежныхСредств.Наличные)) КАК ДенежныеОстаткиКассы
		ПО (НАЧАЛОПЕРИОДА(Документ.Дата, ДЕНЬ) = ДенежныеОстаткиКассы.ПериодДень)
			И Документ.Организация = ДенежныеОстаткиКассы.Организация
			И Документ.Касса = ДенежныеОстаткиКассы.БанковскийСчетКасса</QueryText>
				<MainTable>Document.КассоваяКнига</MainTable>
